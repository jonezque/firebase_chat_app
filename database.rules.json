{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    ".read": true,
    ".write": false,
      
    "users": {
      "$user": {
        // grants write access to the owner of this user account
        // whose uid must exactly match the key ($user_id)
        ".write": "$user === auth.uid"
      }
    },
    
    "channels": {
      ".write": true
    }
  }
}